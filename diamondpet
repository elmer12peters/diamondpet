    }</style> <script>function categoryaccordion(e) {
        var activeaccordion = e.dataset.acc;
        var categoryaccordion = document.getElementsByClassName('accordionSub');
        var categoryLabel = document.getElementsByClassName('accordionLabel');
		var winWidth = window.innerWidth;
        for (var i = 0; i < categoryaccordion.length; i++) {
            if(categoryaccordion[i].id == activeaccordion) {
                if(!categoryaccordion[i].classList.contains('active')) {
                    categoryaccordion[i].classList.add('active');
                    categoryaccordion[i].style.maxHeight = categoryaccordion[i].scrollHeight + "px";
                } else {
                    categoryaccordion[i].classList.remove('active');
                    categoryaccordion[i].style.maxHeight = 0;
                }
            } else {
                categoryaccordion[i].classList.remove('active');
                	categoryaccordion[i].style.maxHeight = 0;
            }
        }
        for (var i = 0; i < categoryLabel.length; i++) {
            if(categoryLabel[i] == e) {
                if(!categoryLabel[i].classList.contains('active')) {
                    categoryLabel[i].classList.add('active');
                } else {
                    categoryLabel[i].classList.remove('active');
                }
            } else {
                categoryLabel[i].classList.remove('active');
            }
        }
    }

    function mobileMenuAccordion() {
        const menuItems = document.querySelectorAll('.navRight nav > ul > li > span');
        const menuTop = document.querySelectorAll('.navRight nav > ul');
		    var winWidth = window.innerWidth;
        menuItems.forEach(li => li.addEventListener('click', () => {
            var topLevelItem = li.parentElement; 
            var subMenu = topLevelItem.querySelector('.subMenu');
            // Close all other subMenus if they aren't the child of the clicked li
            menuTop[0].scroll(0,(li.offsetTop) + 5);
            document.querySelectorAll('.navRight nav ul li .subMenu').forEach(_subMenu => {
                if (subMenu == _subMenu) return;
                _subMenu.classList.remove('active');
				        topLevelItem.classList.remove('active');
            });
            // Add the active class if the current subMenu didn't have it, or remove it if it was already there
        if (subMenu.classList.contains('active')) {
				  topLevelItem.classList.remove('active');
          subMenu.classList.remove('active');
          if(winWidth < 1250) {
            //subMenu.style.maxHeight = 0;
          }
          menuTop[0].scroll(0,(li.offsetTop) + 5);
        } else {
          var topLevel = document.querySelectorAll('.navRight nav ul .topLevel');
          var topLength = topLevel.length;
          for (var i = 0; i < topLength; i++) {
            if(topLevel[i] == topLevelItem){
              topLevelItem.classList.add('active');
            } else {
              topLevel[i].classList.remove('active');
            }
          }
          subMenu.classList.add('active');
          if(winWidth < 1250) {
              //subMenu.style.maxHeight = subMenu.scrollHeight + "px";
          }
          menuTop[0].scroll(0,(li.offsetTop) + 5);
        }
        
        }))
    }

    mobileMenuAccordion();
    
    
    //FollowNavHideTillScrollUp
    let position = "absolute";
    let navbarTop = 0;
    let lastScrollPosition = 0;
    let changedPosition = false;
    const supportPageOffset = window.pageYOffset !== undefined;
    
    function onScroll() {
      const navbar = document.getElementById("mainNavigation");
      //const navbarTab = document.getElementById("tab-alert");
      const currentScrollPosition = supportPageOffset
        ? window.pageYOffset
        : document.documentElement.scrollTop;
      if (currentScrollPosition <= 0) {
        if (position !== "absolute") {
          changedPosition = true;
        } else {
          changedPosition = false;
        }
        position = "absolute";
        navbarTop = 0;
        lastScrollPosition = 0;
      } else {
        if (currentScrollPosition > lastScrollPosition) {
          if (position !== "absolute") {
            changedPosition = true;
          } else {
            changedPosition = false;
          }
          position = "absolute";
          let { top, height } = navbar.getBoundingClientRect();
          if (top < -height) {
            top = -height;
          }
          navbarTop = currentScrollPosition + top;
        } else if (currentScrollPosition < lastScrollPosition) {
          const { top } = navbar.getBoundingClientRect();
          if (top >= 0) {
            navbarTop = 0;
            if (position !== "fixed") {
              changedPosition = true;
            } else {
              changedPosition = false;
            }
            position = "fixed";
          }
        }
        lastScrollPosition = currentScrollPosition;
      }
      navbar.style = `position: ${position}; top: ${navbarTop}px; transitiona: ${
        changedPosition ? "none" : "100ms linear"
      }`;
    }
    
    window.addEventListener("scroll", onScroll, { passive: true });</script> <style type="text/css">/*Nav 2022*/
